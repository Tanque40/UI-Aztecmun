<div class="card">
    <span id="tituloVotacion" class="card-title"></span>
    <div class="card-content">
        <div class="row">
            <div class="col s6">
                <%= select_tag (:propone) do%>
                    <% presentes.each do |s|%>
                        <%= content_tag(:option, s.pais, value: s.pais) %>
                    <% end %>
                    <%= label_tag :propone%>
                <% end %>
            </div>
            <div class="col s6">
                <%= select_tag(:secunda) do%>
                    <% presentes.each do |s|%>
                        <%= content_tag(:option, s.pais, value: s.pais) %>
                    <% end %>
                    <%= label_tag :secunda%>
                <% end %>
            </div>
        </div>
    </div>
</div>
