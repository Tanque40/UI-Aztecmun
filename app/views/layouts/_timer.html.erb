<div id="timerTest">
    <div class="row">
        <div class="col s6">
            <label for="minutos" class="left">Minutos</label>
            <input type="number" id="minutos" name="minutos" placeholder="minutos" value="0">
        </div>
        <div class="col s6">
            <label for="segundos" class="left">Segundos</label>
            <input type="number" id="segundos" name="segundos" placeholder="segundos" value="0">
        </div>
    </div>
    <button type="button" name="button" onclick="temporizador()" data-remote="true" class="waves-effect waves-light btn">Iniciar</button>
    <button type="button" id="detener" name="button" onclick="detener()" data-remote="true" class="waves-effect waves-light btn">Detener</button>
    <button type="button" name="button" onclick="reset()" data-remote="true" class="waves-effect waves-light btn">Resetear</button>
    <h3 id="min">00:00</h3>
    <div class="progress" id="progress">
        <div class="determinate" style="width: 100%; heigth: 100px;"></div>
    </div>
</div>
